.row
  .col-md-8.col-md-offset-1
    = bootstrap_form_for @group do |f|
      %header
        %h1 
          %i.fa.fa-plus-circle
          Create Babysitting Circle
      %br
      = f.text_field :group_name, label: "Circle Name"
      = f.text_field :location
      = f.submit "Create Circle", class: "btn bg-olive btn-flat btn-lg"